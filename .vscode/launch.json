{
    "version": "0.2.0",
    "configurations": [
       
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/dotnetcore/src/Smooth.Console/bin/Debug/netcoreapp1.1/win10-x64/Smooth.exe",
            "args": [ "c:\\temp\\Smooth\\Images", "c:\\temp\\Smooth\\Root", "-s" ],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port":2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}\\golang\\src\\practice\\boring.go",
             "env": {
                  "GOPATH": "${env.GOPATH};${workspaceRoot}\\golang"
            },
             "args": [ ],
             "showLog": true
          }

    ]
}